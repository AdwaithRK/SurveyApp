<h3 style="text-align: center;margin-top: 8px; font-family: cursive;">Entries </h3>

<table class="table front-table table-hover p-3">
    <thead>
        <%@array.first.each do |row|%>
            <th><%=row%></th>
        <%end%>
    </thead>
    <tbody class="m-3 p-3">
        <%@array.slice(1..-1).each do |row|%>
            <tr>
                <%row.each do |col|%>
                    <td><%=col%></td>
                <%end%>
            </tr>
        <%end%>
    </tbody>
</table>