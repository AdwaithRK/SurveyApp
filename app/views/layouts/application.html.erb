<!DOCTYPE html>
<html>
  <head>
    <title>RailsSurveyApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <header class="navbar navbar-fixed-top dark-black-color nav-style">
       <div class="col-sm-12 main-header-div">
          <%if user_signed_in? %>
              <%= render 'layouts/logout_header'%>
          <%else%>
            <%= render 'layouts/login_header' %>
          <%end%>
      </div>
    </header>




    <%= yield %>
  </body>
</html>
